# Lynxa Hub - Environment Configuration

# Backend API Configuration
REACT_APP_BACKEND_URL=https://lynxa-pro-backend.vercel.app
REACT_APP_API_VERSION=v1

# Authentication Configuration (if migrating to Clerk)
# REACT_APP_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Application Configuration
REACT_APP_APP_NAME=Nexariq AI Platform
REACT_APP_APP_VERSION=2.0.0
REACT_APP_ENVIRONMENT=development

# Features Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_BILLING=true
REACT_APP_ENABLE_PLAYGROUND=true

# API Endpoints
REACT_APP_LYNXA_API_ENDPOINT=/api/lynxa
REACT_APP_KEYS_API_ENDPOINT=/api/keys
REACT_APP_USERS_API_ENDPOINT=/api/users
REACT_APP_BILLING_API_ENDPOINT=/api/billing

# WebSocket Configuration
REACT_APP_WS_URL=wss://lynxa-pro-backend.vercel.app
REACT_APP_WS_PORT=8080

# Development Configuration
REACT_APP_DEBUG=false
REACT_APP_MOCK_DATA=false

# Deployment Configuration
REACT_APP_CDN_URL=https://cdn.nexariq.com
REACT_APP_ASSETS_URL=/assets

# Analytics
REACT_APP_GA_TRACKING_ID=
REACT_APP_MIXPANEL_TOKEN=

# Support & Documentation
REACT_APP_DOCS_URL=https://docs.nexariq.com
REACT_APP_SUPPORT_URL=https://support.nexariq.com
REACT_APP_STATUS_URL=https://status.nexariq.com